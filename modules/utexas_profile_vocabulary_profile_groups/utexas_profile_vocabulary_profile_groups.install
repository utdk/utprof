<?php

/**
 * @file
 * Install/uninstall functions for the UTexas Profile Groups module.
 */

/**
 * Implements hook_uninstall().
 */
function utexas_profile_vocabulary_profile_groups_uninstall() {
  $config_factory = \Drupal::configFactory();

  // Remove the vocabulary that this module installed.
  $config_factory->getEditable('taxonomy.vocabulary.utexas_profile_groups')
    ->delete();
}
